{
	"name": "Twinty",
	"version": "1.0",
	"manifest_version": 2,
	"description": "We limit your tweets",
	"browser_action": {
		"default_icon": "twinty_icon.png",
		"default_title": "You are using Twinty with Twitter"
	},
	"content_scripts": [
    {
      "matches": ["https://www.twitter.com/*"],
      "js": ["twinty.js"]
    }
  ],
}